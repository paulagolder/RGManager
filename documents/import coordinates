

UPDATE `roadgroup` rg, `TABLE 16` t SET rg.`distance`=t.distance ,rg.`kml`=t.kml,rg.`minlat`=t.minlat,rg.`maxlat`=t.maxlat,rg.`minlong`=t.minlong,rg.`maxlong`=t.maxlong WHERE rg.roadgroupid = t.roadgroupid and rg.year = t.year



